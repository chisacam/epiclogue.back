{
  "name": "lunacat-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:prod": "yarn build && pm2-runtime start ecosystem.config.js",
    "dev": "yarn build && nodemon build/bin/www",
    "babel": "babel-node ./bin/www",
    "build": "babel \"src/\" -d \"build/\"",
    "docker": "yarn build && nodemon build/bin/www",
    "format": "prettier --write \"src/**/*.js\" \"__tests__/**/*.js\"",
    "lint": "eslint --fix \"src/**/*.js\" \"__tests__/**/*.test.js\"",
    "lint:init": "eslint --init",
    "test": "NODE_ENV=test && ./node_modules/.bin/jest tests --config=jest.config.js --runInBand",
    "test:debug": "NODE_ENV=test &&./node_modules/.bin/jest tests --config=jest.config.js --runInBand --detectOpenHandles ",
    "test:docker": "docker exec -it server yarn test"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@slack/webhook": "^6.0.0",
    "aws-sdk": "^2.859.0",
    "axios": "^0.21.1",
    "connect-redis": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "~4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "mongoose": "^5.12.3",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.6",
    "redis": "^3.1.0",
    "slack-node": "^0.1.8",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.15",
    "@jest/globals": "^26.6.2",
    "@shelf/jest-mongodb": "^1.2.2",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "form-data": "^4.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "random-string": "^0.2.0",
    "supertest": "^6.1.3"
  }
}
