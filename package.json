{
  "name": "lunacat-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:prod": "yarn build && pm2-runtime start ecosystem.config.js",
    "dev": "yarn build && nodemon build/bin/www",
    "babel": "babel-node ./bin/www",
    "prebuild": "yarn",
    "build": "babel \"src/\" -d \"build/\"",
    "docker": "yarn build && nodemon build/bin/www",
    "format": "prettier --write \"src/**/*.js\" \"__tests__/**/*.js\"",
    "lint": "eslint --fix \"src/**/*.js\" \"__tests__/**/*.test.js\"",
    "lint:init": "eslint --init",
    "test": "NODE_ENV=test MONGOMS_VERSION=4.4.0 ./node_modules/.bin/jest tests --config=jest.config.js --runInBand",
    "test:debug": "NODE_ENV=test MONGOMS_VERSION=4.4.0 ./node_modules/.bin/jest tests --config=jest.config.js --runInBand --detectOpenHandles",
    "test:docker": "docker exec -it server yarn test",
    "test:echo": "echo `hello npm test!",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@slack/webhook": "^6.0.0",
    "aws-sdk": "^2.1089.0",
    "axios": "^0.26.0",
    "connect-redis": "^6.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.8",
    "debug": "~4.3.1",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "express": "~4.17.3",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.4.1",
    "mongodb-memory-server": "^8.4.0",
    "mongoose": "^6.2.4",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.6.2",
    "nodemon": "^2.0.7",
    "pm2": "^5.2.0",
    "redis": "^4.0.4",
    "slack-node": "^0.1.8",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@jest/globals": "^27.5.1",
    "@shelf/jest-mongodb": "^2.2.1",
    "@types/express": "^4.17.12",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-jest": "^27.5.1",
    "babel-preset-env": "^1.7.0",
    "cookie": "^0.4.2",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "form-data": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "random-string": "^0.2.0",
    "supertest": "^6.1.3"
  }
}
